# Azure B2C URLs
VITE_B2C_URL_LOGIN=https://your-tenant.b2clogin.com/your-tenant.onmicrosoft.com/B2C_1A_SIGNIN/oauth2/v2.0/authorize
VITE_B2C_URL_SIGNUP=https://your-tenant.b2clogin.com/your-tenant.onmicrosoft.com/B2C_1A_SIGNUP/oauth2/v2.0/authorize
VITE_B2C_URL_PWD=https://your-tenant.b2clogin.com/your-tenant.onmicrosoft.com/B2C_1A_PASSWORDRESET/oauth2/v2.0/authorize

# Base URL for deployment
VITE_URL_BASE=/

# Domain for preview
VITE_URL_DOMAIN=localhost

# Google Tag Manager
VITE_GTM_ID=GTM-XXXXXXX

# reCAPTCHA
VITE_RECAPTCHA_SITE_KEY=your-recaptcha-site-key
VITE_RECAPTCHA_ENABLE_LOGIN=0
VITE_RECAPTCHA_ENABLE_REGISTER=0

# Facebook Pixel
VITE_FB_ID=your-facebook-pixel-id
