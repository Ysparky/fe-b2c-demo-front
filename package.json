{
  "name": "pacifico-b2c-web-empresa",
  "version": "1.0.0",
  "description": "Azure B2C Custom UI for Pacifico Web Empresa - 6 Scenarios",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "build:skip-check": "vite build",
    "build:demo": "vite build && cp build/scenarios/scenario-demo-basic-login/demo-login.html build/customize-ui.html && mkdir -p build/assets/images && cp src/assets/images/logo.png build/assets/images/logo.png",
    "preview": "vite preview --port 3000",
    "watch": "vite build --watch & vite preview --port 3000",
    "lint": "eslint src --ext ts,tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["azure-b2c", "typescript", "vite", "pacifico"],
  "author": "Pacifico Seguros",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "postcss": "^8.4.49",
    "rimraf": "^6.0.1",
    "typescript": "~5.6.2",
    "vite": "^6.2.2",
    "vite-plugin-minify": "^2.1.0",
    "vite-plugin-nunjucks": "^0.2.0"
  },
  "dependencies": {
    "yup": "^1.6.1"
  }
}
